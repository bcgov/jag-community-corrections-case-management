{
    "display": "form",
    "components": [
        {
            "label": "",
            "key": "key_well",
            "type": "well",
            "input": false,
            "tableView": false,
            "components": [
                {
                    "legend": "${content_title}",
                    "tooltip": "${tooltip_title}",
                    "key": "${key_well}",
                    "type": "fieldset",
                    "label": "",
                    "input": false,
                    "tableView": false,
                    "components": [
                        {
                            "label": "HTML",
                            "attrs": [
                                {
                                    "attr": "",
                                    "value": ""
                                }
                            ],
                            "content": "${content_html}",
                            "refreshOnChange": false,
                            "key": "key_htmlContent",
                            "type": "htmlelement",
                            "input": false,
                            "tableView": false
                        },
                        {
                            "label": "",
                            "tableView": false,
                            "key": "${key_container}",
                            "type": "container",
                            "input": true,
                            "components": [
                                {
                                    "label": "${label_checkbox}",
                                    "tooltip": "${tooltip_checkbox}",
                                    "tableView": false,
                                    "defaultValue": false,
                                    "key": "${key_checkbox}",
                                    "type": "checkbox",
                                    "input": true
                                },
                                {
                                    "label": "${label_textarea}",
                                    "autoExpand": false,
                                    "tableView": true,
                                    "inputFormat": "plain",
                                    "validate": {
                                        "maxLength": "${maxLength_textarea}"
                                    },
                                    "key": "${key_textarea}",
                                    "conditional": {
                                        "show": true,
                                        "when": "${key_checkbox}",
                                        "eq": "true"
                                    },
                                    "type": "textarea",
                                    "input": true
                                },
                                {
                                  "label": "",
                                  "tableView": true,
                                  "hide": true,
                                  "clearOnHide": false,
                                  "key": "hidden_key",
                                  "defaultValue": "${value_hiddenKey}",
                                  "type": "textfield",
                                  "input": true
                                },
                                {
                                    "label": "",
                                    "action": "event",
                                    "customClass": "hide",
                                    "attributes": { 
                                        "id": "${key_container}" 
                                    },
                                    "hide": false,
                                    "showValidations": false,
                                    "disableOnInvalid": true,
                                    "tableView": true,
                                    "key": "key_submit",
                                    "type": "button",
                                    "input": false,
                                    "event": "evt_submitBtnClicked"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}