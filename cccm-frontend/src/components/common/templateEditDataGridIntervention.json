{
  "display": "form",
  "components": [
    {
      "label": "${label_editgrid}",
      "key": "questionCombo",
      "type": "editgrid",
      "input": "true",
      "templates": {
        "header": "\n<div class=\"row\"> \n  {% util.eachComponent(components, function(component) { %}  \n    <div class=\"col-sm-2\">\n      <strong>{{ component.label }}</strong> \n    </div> \n  {% }) %} \n</div>",
        "row": "\n<div class=\"row\"> \n  {%util.eachComponent(components, function(component) { %} \n    <div class=\"col-sm-2\">\n      {{ row[component.key] }}\n    </div> \n  {% }) %} \n  <div class=\"col-sm-2\"> \n    <div class=\"btn-group pull-right\"> \n      <div class=\"btn btn-default btn-sm editRow\"><i class=\"fa fa-edit\"></i></div> \n       \n    </div>  \n  </div> \n</div>",
        "footer": ""
      },
      "disableAddingRemovingRows": true,
      "components": [
        {
          "label": "Columns",
          "columns": [
              {
                "components": [
                  {
                    "label": "Criminogenic Needs",
                    "tableView": true,
                    "key": "questionLabel",
                    "type": "textfield",
                    "input": true,
                    "disabled": true,
                    "defaultValue": ""
                  }
                ],
                "width": 3,
                "offset": 0,
                "push": 0,
                "pull": 0,
                "size": "md",
                "currentWidth": 3
              },
              {
                "components": [
                  {
                    "label": "Specific Factor",
                    "tableView": true,
                    "key": "comments",
                    "type": "textfield",
                    "input": true,
                    "defaultValue": ""
                  }
                ],
                "width": 3,
                "offset": 0,
                "push": 0,
                "pull": 0,
                "size": "md",
                "currentWidth": 3
              },
              {
                "components": [
                    {
                        "label": "Intervention Type",
                        "tableView": true,
                        "key": "key_itv_type",
                        "type": "textfield",
                        "disabled": true,
                        "input": true
                    }
                  ],
                  "width": 3,
                  "offset": 0,
                  "push": 0,
                  "pull": 0,
                  "size": "md",
                  "currentWidth": 3
              },
              {
                "components": [
                    {
                        "label": "Intervention Description",
                        "tableView": true,
                        "key": "key_itv_description",
                        "type": "textfield",
                        "input": true
                    }
                  ],
                  "width": 3,
                  "offset": 0,
                  "push": 0,
                  "pull": 0,
                  "size": "md",
                  "currentWidth": 3
              }
          ],
          "key": "columns",
          "type": "columns",
          "input": false,
          "tableView": false
        }
      ]
    }
  ]
}