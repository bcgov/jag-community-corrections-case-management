{
  "display": "form",
  "components": [
    {
        "label": "",
        "key": "key_well",
        "type": "well",
        "input": false,
        "tableView": false,
        "components": [
          {
            "label": "HTML",
            "className": "${className_label}",
            "attrs": [
                {
                    "attr": "",
                    "value": ""
                }
            ],
            "content": "${label_editgrid}",
            "refreshOnChange": false,
            "key": "key_title",
            "type": "htmlelement",
            "input": false,
            "tableView": false
          },
          {
            "type": "editgrid",
            "key": "${key_editgrid}",
            "label": "",
            "input": "true",
            "templates": {
              "header": "\n<div class=\"row justify-content-between\"> \n  {% util.eachComponent(components, function(component) { %}  \n    <div class=\"col-sm-2\">\n      <strong>{{ component.label }}</strong> \n    </div> \n  {% }) %} \n <div class=\"col-sm-3\">\n <strong>Actions</strong>\n</div> \n</div>",
              "row": "\n<div class=\"row justify-content-around\"> \n  {%util.eachComponent(components, function(component) { %} \n    <div class=\"col-sm-2\">\n      {{ row[component.key] }}\n    </div> \n  {% }) %} \n  <div class=\"col-sm-2 text-left\"> \n    <div class=\"btn-group\"> \n      <div class=\"btn btn-default btn-sm editRow\"><i class=\"fa fa-edit\"></i></div> \n       \n    </div>  \n  </div> \n</div>",
              "footer": ""
            },
            "disableAddingRemovingRows": true,
            "components": [
              {
                "label": "Columns",
                "columns": [
                    {
                      "components": [
                        {
                          "label": "Criminogenic Needs",
                          "tableView": true,
                          "key": "${key_questionLabel}",
                          "type": "textfield",
                          "input": true,
                          "disabled": true,
                          "defaultValue": ""
                        }
                      ],
                      "width": 3,
                      "offset": 0,
                      "push": 0,
                      "pull": 0,
                      "size": "md",
                      "currentWidth": 3
                    },
                    {
                      "components": [
                        {
                          "label": "Specific Factor",
                          "tableView": true,
                          "key": "${key_comments}",
                          "type": "textarea",
                          "input": true,
                          "defaultValue": ""
                        }
                      ],
                      "width": 3,
                      "offset": 0,
                      "push": 0,
                      "pull": 0,
                      "size": "md",
                      "currentWidth": 3
                    },
                    {
                      "components": [
                          {
                              "label": "Intervention Type",
                              "tableView": true,
                              "key": "${key_itv_type}",
                              "type": "textfield",
                              "disabled": true,
                              "input": true
                          }
                        ],
                        "width": 3,
                        "offset": 0,
                        "push": 0,
                        "pull": 0,
                        "size": "md",
                        "currentWidth": 3
                    },
                    {
                      "components": [
                          {
                              "label": "Intervention Description",
                              "tableView": true,
                              "key": "${key_itv_description}",
                              "type": "textarea",
                              "input": true
                          }
                        ],
                        "width": 3,
                        "offset": 0,
                        "push": 0,
                        "pull": 0,
                        "size": "md",
                        "currentWidth": 3
                    }
                ],
                "key": "columns",
                "type": "columns",
                "input": false,
                "tableView": false
              }
            ]
          }
        ]
      }
    ]
}