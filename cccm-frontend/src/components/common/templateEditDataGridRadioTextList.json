{
  "display": "form",
  "components": [
    {
      "label": "HTML",
      "content": "${label_editgrid}",
      "className": "${className_label}",
      "refreshOnChange": false,
      "key": "html",
      "type": "htmlelement",
      "input": false,
      "tableView": false
    },
    {
      "label": "Columns",
      "customClass": "editgrid-header",
      "columns": [
        {
          "components": [
              {
              "type": "textfield",
              "label": "Question",
              "tableView": true,
              "key": "${key_questionLabel}",
              "input": true,
              "disabled": true,
              "defaultValue": "",
              "customClass": "hide-input"
            }
          ],
          "offset": 0,
          "push": 0,
          "pull": 0,
          "size": "md",
          "currentWidth": 3,
          "width": 3
        },
        {
          "components": [
            {
              "type": "textfield",
              "label": "Answer",
              "tableView": true,
              "key": "${key_questionLabel}",
              "input": true,
              "disabled": true,
              "defaultValue": "",
              "customClass": "hide-input"
            }
          ],
          "offset": 0,
          "push": 0,
          "pull": 0,
          "size": "md",
          "currentWidth": 3,
          "width": 3
        },
        {
          "components": [
            {
              "type": "textfield",
              "label": "Comment",
              "tableView": true,
              "key": "${key_questionLabel}",
              "input": true,
              "disabled": true,
              "defaultValue": "",
              "customClass": "hide-input"
            }
          ],
          "size": "md",
          "offset": 0,
          "push": 0,
          "pull": 0,
          "currentWidth": 3,
          "width": 3
        },
        {
          "components": [
            {
              "type": "textfield",
              "label": "Actions",
              "tableView": true,
              "key": "crap",
              "input": true,
              "disabled": true,
              "defaultValue": "",
              "customClass": "hide-input"
            }
          ],
          "size": "md",
          "offset": 0,
          "push": 0,
          "pull": 0,
          "currentWidth": 3,
          "width": 3
        }
      ],
      "key": "columns",
      "type": "columns",
      "input": false,
      "tableView": false
    },
    {
      "type": "editgrid",
      "key": "${key_editgrid}",
      "label": "",
      "input": true,
      "hideLabel": false,
      "templates": {
        "header": "\n<div class=\"row\"> \n  {% util.eachComponent(components, function(component)   { %}  \n    <div class=\"col-sm-3\">\n      <strong>{{ component.label }}</strong> \n    </div> \n  {% }) %} \n \n <div class=\"col-sm-3\">\n <strong>Actions</strong>\n</div> </div> ",
        "row": "\n<div class=\"row\"> \n  {%util.eachComponent(components, function(component) { %} \n    <div class=\"col-sm-3\">\n      {{ row[component.key] }}\n    </div> \n  {% }) %} \n  <div class=\"col-sm-3\"> \n    <div class=\"btn-group pull-right\"> \n      <div class=\"btn btn-default btn-sm editRow\"><i class=\"fa fa-edit\"></i></div> \n       \n    </div>  \n  </div> \n</div>",
        "footer": ""
      },
      "disableAddingRemovingRows": true,
      "components": [
        {
          "label": "Columns",
          "columns": [
              {
                "components": [
                  {
                    "type": "textfield",
                    "label": "Question",
                    "tableView": true,
                    "key": "${key_questionLabel}",
                    "input": true,
                    "disabled": true,
                    "defaultValue": ""
                  }
                ],
                "width": 4,
                "offset": 0,
                "push": 0,
                "pull": 0,
                "size": "md",
                "currentWidth": 4
              },
              {
                "components": [
                  {
                    "type": "radio",
                    "label": "Answer",
                    "key": "${key_radioButton}",
                    "optionsLabelPosition": "right",
                    "inline": false,
                    "tableView": false,
                    "values": [],
                    "input": true,
                    "defaultValue": "a"
                  }
                ],
                "width": 4,
                "offset": 0,
                "push": 0,
                "pull": 0,
                "size": "md",
                "currentWidth": 4
              },
              {
                "components": [
                    {
                      "type": "textarea",
                      "label": "Comments",
                      "tableView": true,
                      "key": "${key_comments}",
                      "input": true
                    }
                  ],
                  "width": 4,
                  "offset": 0,
                  "push": 0,
                  "pull": 0,
                  "size": "md",
                  "currentWidth": 4
              }
          ],
          "key": "columns",
          "type": "columns",
          "input": false,
          "tableView": false
        }
      ]
    }
  ]
}