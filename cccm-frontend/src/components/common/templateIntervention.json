{
    "type": "form",
    "components": [
        {
            "label": "${label}",
            "tableView": false,
            "attributes": {
              "id": "${key}"
            },
            "key": "${key}",
            "type": "checkbox",
            "input": true,
            "defaultValue": "${defaultValue}"
        },
        {
            "label": "ivDetails",
            "key": "${key_itv_details}",
            "type": "well",
            "input": false,
