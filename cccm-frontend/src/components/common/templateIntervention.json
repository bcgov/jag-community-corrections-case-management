{
    "type": "form",
    "components": [
        {
            "label": "${label}",
            "tableView": false,
            "attributes": {
              "id": "${key_checkbox}"
            },
            "key": "${key_checkbox}",
            "type": "checkbox",
            "input": true,
            "defaultValue": "${defaultValue_checkbox}"
        },
        {
            "label": "",
            "reorder": false,
            "addAnotherPosition": "bottom",
            "layoutFixed": false,
            "enableRowGroups": false,
            "initEmpty": false,
            "tableView": false,
            "defaultValue": [
                {
                    "${key_itv_type}": "",
                    "${key_itv_other}": "${value_itv_other}",
                    "${key_itv_description}": ""
                }
            ],
            "key": "${key_itvDataGrid}",
            "conditional": {
                "show": true,
                "when": "${key_checkbox}",
                "eq": "true"
            },
            "type": "datagrid",
            "input": true,
            "components": [
                {
                    "collapsible": false,
                    "key": "${key_itvPanel}",
                    "type": "well",
                    "label": "",
                    "input": false,
                    "tableView": false,
                    "components": [
                        {
                            "label": "Columns",
                            "columns": [
                                {
                                    "components": [
                                        {
                                            "label": "Select Intervention Type",
                                            "widget": "choicesjs",
                                            "tableView": true,
                                            "key": "${key_itv_type}",
                                            "type": "select",
                                            "input": true,
                                            "data": {
                                                "values": [
                                                    {
                                                        "label": "",
                                                        "value": ""
                                                    }
                                                ]
                                            }
                                        }
                                    ],
                                    "width": 6,
                                    "offset": 0,
                                    "push": 0,
                                    "pull": 0,
                                    "size": "md",
                                    "currentWidth": 6
                                },
                                {
                                    "components": [
                                        {
                                            "label": "Specify",
                                            "tableView": true,
                                            "key": "${key_itv_other}",
                                            "conditional": {
                                                "show": true,
                                                "when": "${key_itvDataGrid}.${key_itv_type}",
                                                "eq": "other"
                                            },
                                            "defaultValue": "${value_itv_other}",
                                            "type": "textfield",
                                            "input": true
                                        }
                                    ],
                                    "width": 6,
                                    "offset": 0,
                                    "push": 0,
                                    "pull": 0,
                                    "size": "md",
                                    "currentWidth": 6
                                }
                            ],
                            "key": "columns",
                            "type": "columns",
                            "input": false,
                            "tableView": false
                        },
                        {
                            "label": "Description",
                            "autoExpand": false,
                            "tableView": true,
                            "placeholder": "Please select an intervention type",
                            "key": "${key_itv_description}",
                            "logic": [
                                {
                                    "name": "diableLogic",
                                    "trigger": {
                                        "type": "simple",
                                        "simple": {
                                            "show": true,
                                            "when": "${key_itvDataGrid}.${key_itv_type}",
                                            "eq": ""
                                        }
                                     },
                                    "actions": [
                                        {
                                            "name": "disableAction",
                                            "type": "property",
                                            "property": {
                                                "label": "Hidden",
                                                "value": "hidden",
                                                "type": "boolean"
                                            },
                                            "state": "true"
                                        }
                                    ]
                                }
                            ],
                            "type": "textarea",
                            "input": true
                        }
                    ]
                }
            ]
        }
    ]
}