{
    "type": "form",
    "components": [
        {
            "label": "formDataEntry",
            "key": "formDataEntry",
            "type": "well",
            "input": false,
            "tableView": false,
            "components": [
              {
                "label": "${questionLabel}",
                "optionsLabelPosition": "right",
                "inline": false,
                "tableView": false,
                "values": [
                    {
                        "label": "Pattern of stable and supportive relationships",
                        "value": "patternOfStableAndSupportiveRelationships",
                        "shortcut": "P"
                    },
                    {
                        "label": "No current difficulties",
                        "value": "noCurrentDifficulties",
                        "shortcut": "N"
                    },
                    {
                        "label": "Occasional Instability in relationships",
                        "value": "occasionalInstabilityInRelationships",
                        "shortcut": "O"
                    },
                    {
                        "label": "Very unstable pattern of relationships",
                        "value": "veryUnstablePatternOfRelationships",
                        "shortcut": "V"
                    }
                ],
                "key": "radioButton",
                "type": "radio",
                "input": true,
                "defaultValue": "${defaultValue_radioButton}"
              },
              {
                "label": "",
                "tableView": true,
                "hidden": true,
                "clearOnHide": false,
                "key": "hidden_key",
                "defaultValue": "${key_radioButton}",
                "type": "textfield",
                "input": true
              },
              {
                  "label": "Comments",
                  "placeholder": "Please enter here",
                  "description": "",
                  "tooltip": "",
                  "autoExpand": true,
                  "tableView": true,
                  "key": "comments",
                  "type": "textarea",
                  "input": true,
                  "defaultValue": "${defaultValue_comments}"
              },
              {
                "label": "${label}",
                "tableView": false,
                "key": "key_checkbox",
                "type": "checkbox",
                "input": true,
                "defaultValue": "${defaultValue_checkbox}"
              },
              {
                  "label": "",
                  "reorder": false,
                  "addAnotherPosition": "bottom",
                  "layoutFixed": false,
                  "enableRowGroups": false,
                  "initEmpty": false,
                  "tableView": false,
                  "defaultValue": [
                      {
                          "key_itv_type": "",
                          "key_itv_other": "${value_itv_other}",
                          "key_itv_description": ""
                      }
                  ],
                  "key": "key_itvDataGrid",
                  "conditional": {
                      "show": true,
                      "when": "key_checkbox",
                      "eq": "true"
                  },
                  "type": "datagrid",
                  "input": true,
                  "components": [
                      {
                          "collapsible": false,
                          "key": "panel_itv",
                          "type": "well",
                          "label": "",
                          "input": false,
                          "tableView": false,
                          "components": [
                              {
                                  "label": "Columns",
                                  "columns": [
                                      {
                                          "components": [
                                              {
                                                  "label": "Select Intervention Type",
                                                  "widget": "choicesjs",
                                                  "tableView": true,
                                                  "key": "key_itv_type",
                                                  "type": "select",
                                                  "input": true,
                                                  "data": {
                                                      "values": [
                                                          {
                                                              "label": "",
                                                              "value": ""
                                                          }
                                                      ]
                                                  }
                                              }
                                          ],
                                          "width": 6,
                                          "offset": 0,
                                          "push": 0,
                                          "pull": 0,
                                          "size": "md",
                                          "currentWidth": 6
                                      },
                                      {
                                          "components": [
                                              {
                                                  "label": "Specify",
                                                  "tableView": true,
                                                  "key": "key_itv_other",
                                                  "conditional": {
                                                      "show": true,
                                                      "when": "key_itv_type",
                                                      "eq": "other"
                                                  },
                                                  "defaultValue": "${value_itv_other}",
                                                  "type": "textfield",
                                                  "input": true
                                              }
                                          ],
                                          "width": 6,
                                          "offset": 0,
                                          "push": 0,
                                          "pull": 0,
                                          "size": "md",
                                          "currentWidth": 6
                                      }
                                  ],
                                  "key": "columns",
                                  "type": "columns",
                                  "input": false,
                                  "tableView": false
                              },
                              {
                                  "label": "Description",
                                  "autoExpand": false,
                                  "tableView": true,
                                  "placeholder": "Please select an intervention type",
                                  "key": "key_itv_description",
                                  "type": "textarea",
                                  "input": true
                              }
                          ]
                      }
                  ]
              },
              {
                "label": "",
                "tableView": true,
                "hidden": true,
                "clearOnHide": false,
                "key": "questionLabel",
                "defaultValue": "${questionLabel}",
                "type": "textfield",
                "input": true
              }
            ]
          }
    ]
}