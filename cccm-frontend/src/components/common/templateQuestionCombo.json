{
    "type": "form",
    "components": [
      {
        "label": "formDataEntry",
        "key": "formDataEntry",
        "type": "well",
        "input": false,
        "tableView": false,
        "components": [
          {
            "label": "${questionLabel}",
            "optionsLabelPosition": "right",
            "inline": false,
            "tableView": false,
            "values": [
                {
                    "label": "Pattern of stable and supportive relationships",
                    "value": "patternOfStableAndSupportiveRelationships",
                    "shortcut": "P"
                },
                {
                    "label": "No current difficulties",
                    "value": "noCurrentDifficulties",
                    "shortcut": "N"
                },
                {
                    "label": "Occasional Instability in relationships",
                    "value": "occasionalInstabilityInRelationships",
                    "shortcut": "O"
                },
                {
                    "label": "Very unstable pattern of relationships",
                    "value": "veryUnstablePatternOfRelationships",
                    "shortcut": "V"
                }
            ],
            "key": "${key}",
            "type": "radio",
            "input": true,
            "defaultValue": "${defaultValue}"
          },
          {
              "label": "Comments",
              "placeholder": "Please enter here",
              "description": "",
              "tooltip": "",
              "autoExpand": true,
              "tableView": true,
              "key": "${key}",
              "type": "textarea",
              "input": true
          },
          {
              "label": "Intervention needed",
              "tableView": false,
              "key": "${key_itv_needed}",
              "type": "checkbox",
              "input": true,
              "defaultValue": "${defaultValue_ivNeeded}"
          },
          {
              "label": "ivDetails",
              "key": "${key_itv_details}",
              "type": "well",
              "input": false,
              "tableView": false,
              "components": [
                  {
                      "label": "Columns",
                      "columns": [
                          {
                              "components": [
                                  {
                                      "label": "Select",
                                      "widget": "choicesjs",
                                      "tableView": true,
                                      "key": "${key_itv_type}",
                                      "type": "select",
                                      "input": true,
                                      "data": {
                                        "values": [
                                            {
                                                "label": "",
                                                "value": ""
                                            }
                                        ]
                                    }
                                  }
                              ],
                              "width": 6,
                              "offset": 0,
                              "push": 0,
                              "pull": 0,
                              "size": "md",
                              "currentWidth": 6
                          },
                          {
                              "components": [
                                  {
                                      "label": "Specify",
                                      "tableView": true,
                                      "key": "${key_itv_other}",
                                      "defaultValue": "${value_itv_other}",
                                      "type": "textfield",
                                      "input": true
                                  }
                              ],
                              "width": 6,
                              "offset": 0,
                              "push": 0,
                              "pull": 0,
                              "size": "md",
                              "currentWidth": 6
                          }
                      ],
                      "key": "columns",
                      "type": "columns",
                      "input": false,
                      "tableView": false
                  },
                  {
                      "label": "description",
                      "autoExpand": false,
                      "placeholder": "Please select an intervention type",
                      "defaultValue": "${value_itv_description}",
                      "tableView": true,
                      "key": "${key_itv_description}",
                      "type": "textarea",
                      "input": true
                  }
              ]
          }
        ]
    
      }
    ]
}