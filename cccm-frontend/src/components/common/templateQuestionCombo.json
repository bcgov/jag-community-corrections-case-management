{
    "type": "form",
    "components": [
        {
            "label": "formDataEntry",
            "key": "formDataEntry",
            "type": "well",
            "input": false,
            "tableView": false,
            "components": [
              {
                "label": "${questionLabel}",
                "optionsLabelPosition": "right",
                "inline": false,
                "tableView": false,
                "values": [],
                "key": "${key_radioButton}",
                "type": "radio",
                "input": true,
                "defaultValue": "${defaultValue_radioButton}"
              },
              {
                "label": "",
                "tableView": true,
                "hidden": true,
                "clearOnHide": false,
                "key": "hidden_key",
                "defaultValue": "${value_hiddenKey}",
                "type": "textfield",
                "input": true
              },
              {
                  "label": "Comments",
                  "placeholder": "Please enter here",
                  "description": "",
                  "tooltip": "",
                  "autoExpand": true,
                  "tableView": true,
                  "key": "${key_comments}",
                  "type": "textarea",
                  "input": true,
                  "defaultValue": "${defaultValue_comments}"
              },
              {
                "label": "${label}",
                "tableView": false,
                "key": "${key_checkbox}",
                "type": "checkbox",
                "input": true,
                "defaultValue": "${defaultValue_checkbox}"
              },
              {
                  "type": "datagrid",
                  "key": "${key_itvDataGrid}",
                  "label": "",
                  "reorder": false,
                  "addAnotherPosition": "bottom",
                  "layoutFixed": false,
                  "enableRowGroups": false,
                  "initEmpty": false,
                  "tableView": false,
                  "conditional": {
                      "show": true,
                      "when": "${key_checkbox}",
                      "eq": "true"
                  },
                  "input": true,
                  "components": [
                      {
                          "collapsible": false,
                          "key": "panel_itv",
                          "type": "well",
                          "label": "",
                          "input": false,
                          "tableView": false,
                          "components": [
                              {
                                  "label": "Columns",
                                  "columns": [
                                      {
                                          "components": [
                                              {
                                                  "label": "Select Intervention Type",
                                                  "widget": "choicesjs",
                                                  "tableView": true,
                                                  "key": "${key_itv_type}",
                                                  "type": "select",
                                                  "input": true,
                                                  "data": {
                                                      "values": [
                                                          {
                                                              "label": "",
                                                              "value": ""
                                                          }
                                                      ]
                                                  }
                                              }
                                          ],
                                          "width": 6,
                                          "offset": 0,
                                          "push": 0,
                                          "pull": 0,
                                          "size": "md",
                                          "currentWidth": 6
                                      },
                                      {
                                          "components": [
                                              {
                                                  "label": "Specify",
                                                  "tableView": true,
                                                  "key": "${key_itv_other}",
                                                  "conditional": {
                                                      "show": true,
                                                      "when": "${key_itv_type}",
                                                      "eq": "other"
                                                  },
                                                  "type": "textfield",
                                                  "input": true
                                              }
                                          ],
                                          "width": 6,
                                          "offset": 0,
                                          "push": 0,
                                          "pull": 0,
                                          "size": "md",
                                          "currentWidth": 6
                                      }
                                  ],
                                  "key": "columns",
                                  "type": "columns",
                                  "input": false,
                                  "tableView": false
                              },
                              {
                                  "label": "Description",
                                  "autoExpand": false,
                                  "tableView": true,
                                  "placeholder": "Please enter here",
                                  "key": "${key_itv_description}",
                                  "type": "textarea",
                                  "input": true
                              }
                          ]
                      }
                  ]
              },
              {
                "label": "",
                "tableView": true,
                "hidden": true,
                "clearOnHide": false,
                "key": "questionLabel",
                "defaultValue": "${questionLabel}",
                "type": "textfield",
                "input": true
              }
            ]
          },
          {
            "label": "",
            "action": "event",
            "attributes": { 
                "id": "${value_hiddenKey}" 
            },
            "hidden": false,
            "customClass": "hide",
            "showValidations": false,
            "disableOnInvalid": true,
            "tableView": true,
            "key": "key_submit",
            "type": "button",
            "input": false,
            "event": "evt_submitBtnClicked"
          }
    ]
}