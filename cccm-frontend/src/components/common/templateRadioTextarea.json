{
    "display": "form",
    "components": [
        {
            "label": "radiowell",
            "key": "${key}",
            "type": "well",
            "input": false,
            "tableView": false,
            "components": [
                {
                    "label": "${label_questionLabel}",
                    "optionsLabelPosition": "right",
                    "inline": false,
                    "tableView": false,
                    "values": [],
                    "key": "${key_radioButton}",
                    "type": "radio",
                    "input": true,
                    "defaultValue": "${defaultValue}"
                },
                {
                    "label": "Comments",
                    "placeholder": "Please enter here ",
                    "description": "",
                    "tooltip": "",
                    "autoExpand": false,
                    "tableView": true,
                    "key": "${key_textarea}",
                    "type": "textarea",
                    "input": true,
                    "defaultValue": "${defaultValue_textarea}"
                },
                {
                  "label": "",
                  "tableView": true,
                  "hidden": true,
                  "clearOnHide": false,
                  "key": "hidden_key",
                  "defaultValue": "${value_hiddenKey}",
                  "type": "textfield",
                  "input": true
                },
                {
                  "label": "",
                  "action": "event",
                  "attributes": { 
                      "id": "${key}" 
                  },
                  "hidden": false,
                  "customClass": "hide",
                  "showValidations": false,
                  "disableOnInvalid": true,
                  "tableView": true,
                  "key": "key_submit",
                  "type": "button",
                  "input": false,
                  "event": "evt_submitBtnClicked"
                }
            ]
        }
    ]
}